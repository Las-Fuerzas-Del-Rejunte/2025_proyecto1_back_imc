generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model imc {
  id        Int      @id @default(autoincrement())
  peso      Float
  altura    Float
  resultado Float
  categoria String
  createdAt DateTime @default(now())
}
